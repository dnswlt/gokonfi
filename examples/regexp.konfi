let template test(s, re) {
    input: s
    regexp: re
    result: regexp_extract(s, re)
}

let template testg(s, re, group) {
    input: s
    regexp: re
    result: regexp_extract(s, re, group)
}

{
    ex1: test("foo baar baz", "ba*r")
    ex2: testg("Hi, John, how are you?", "Hi, (\\w+),", 1)
}
